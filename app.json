{
  "expo": {
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash-screen.png",
          "resizeMode": "native",
          "dark": {
            "image": "./assets/splash-screen.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 400
        }
      ]
    ],
    "name": "TeToca",
    "icon": "./assets/favicon.png",
    "slug": "TeToca",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "tetoca",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jhonAQ.TeTocaApp"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "navigationBarColor": "#FFFFFF",
      "navigationBarStyle": "light",
      "softwareNavBarColor": "#FFFFFF",
      "package": "com.jhonAQ.TeTocaApp"
    },
    "extra": {
      "eas": {
        "projectId": "da18d90e-bcd5-4688-bc7d-ea22edc6761d"
      }
    }
  }
}
