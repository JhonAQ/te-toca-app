{
  "expo": {
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash-screen.png",
          "resizeMode": "native",
          "dark": {
            "image": "./assets/splash-screen.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 400
        }
      ],
      "expo-web-browser"
    ],
    "name": "TeToca",
    "icon": "./assets/favicon.png",
    "slug": "TeToca",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "navigationBarColor": "#FFFFFF",
      "navigationBarStyle": "light",
      "softwareNavBarColor": "#FFFFFF",
      "package": "com.jhonAQ.TeTocaApp"
    },
    "extra": {
      "eas": {
        "projectId": "da18d90e-bcd5-4688-bc7d-ea22edc6761d"
      }
    },
    "web": {
      "favicon": "./assets/TeTocaLogo.png",
      "bundler": "metro",
      "output": "static",
      "name": "TeToca - Gestión de Colas",
      "shortName": "TeToca",
      "description": "Aplicación para gestionar colas y turnos en instituciones",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#883EF2",
      "backgroundColor": "#0D0426"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "web"
    ]
  }
}
